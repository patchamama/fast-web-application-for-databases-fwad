<type>Object</type>

<description><![CDATA[Mouseover highlight on rows and cells]]></description>

<syntax>xTableCursor(id, inh, def, hov, sel)</syntax>

<parameters>
  <par>
    <name>id</name>
    <note><![CDATA[table id]]></note>
  </par>
  <par>
    <name>inh</name>
    <note><![CDATA[inherit style]]></note>
  </par>
  <par>
    <name>def</name>
    <note><![CDATA[default style]]></note>
  </par>
  <par>
    <name>hov</name>
    <note><![CDATA[row hover style]]></note>
  </par>
  <par>
    <name>sel</name>
    <note><![CDATA[cell selected style]]></note>
  </par>
</parameters>

<properties>
  <prop>
    <name></name>
    <note><![CDATA[]]></note>
  </prop>
</properties>

<dependencies>
  <dep>xGetElementById</dep>
  <dep>xTableIterate</dep>
</dependencies>

<demos>
