<?xml version="1.0"?>
<Main>
  <connection>
    <inc>cocktails/db.xml</inc>
  </connection>
  <configuration>
    <lang value="es">Espa%F1ol</lang>
    <lang value="en">English</lang>
  </configuration>
  <tables>
    <table name="marcas" fieldid="Marca-ID" id="Marca-ID" pk="Marca-ID" autonumeric="ID:gen_marcasid"/>
  </tables>
  <searchs lang="es">
    <sql tableid="marcas" fieldid="Marca-ID" msg="Marcas%3A" id="ID" desc="Nombre" orderby="Nombre">select+%2A+from+marcas+</sql>
  </searchs>
  <elements lang="es">
    <section type="textbox">Marcas</section>
    <element type="textbox" table="marcas" field="Nombre" len="50" lenedit="30" mandatory="true">Nombre%3A</element>
    <element type="textbox" table="marcas" field="Cantidad" len="30" lenedit="30">Cantidad%3A</element>
    <element type="textbox" table="marcas" field="Unidad" len="30" lenedit="30">Unidad%3A</element>
    <element type="textbox" table="marcas" field="Precio" len="4" lenedit="4">Precio%2FUnidad%3A</element>
    <element type="memo" table="marcas" field="Comentario" len="50" lines="10">Comentarios%3A</element>
    <element type="hidden" table="marcas" field="Marca-ID"/>
  </elements>
</Main>